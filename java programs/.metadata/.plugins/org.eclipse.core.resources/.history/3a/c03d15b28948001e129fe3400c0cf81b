package MethodArraysCombined;

class Sample {
	Sample(int a) {
		int fib[] = new int[a+1];
		fib[0] = 0;
		fib[1] = 1;
		for (int i = 2; i <= a; i++) {
			fib[i] = fib[i - 1] + fib[i - 2];
		}
		for (int j = 1; j < fib.length; j++) {
			System.out.print(fib[j] + " ");
		}

	}

}

public class Fibonacci {

	public static void main(String[] args) {
		Sample s = new Sample(20);

	}

}
