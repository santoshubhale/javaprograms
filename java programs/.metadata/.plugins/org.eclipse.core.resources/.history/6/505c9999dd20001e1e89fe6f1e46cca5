package com.java.src;

public class Palindrome {
	public static void main(String[] args) {
		int number = 121;
		int temp = number;
		int rev = 0;

		while (number > 0) {
			int last = number % 10;
			 number = number / 10;
			rev = (rev * 10) + last;
		}
			if(temp==rev ) {
				System.out.println(temp+"is palindrome");
			}else {
				System.out.println(temp+"is not palindrome");
			}

		}

	}


