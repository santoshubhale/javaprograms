package com.java.src;

public class Practice {

	String name;
	int age;
	public Practice(String n , int a) {
		name =n;
		age=a;
	}
	public void display() {
		System.out.println(name);
		System.out.println(age);
	}
	public static void main(String[] args) {
		Practice p = new Practice("santosh", 20);
		p.display();
	}

}
