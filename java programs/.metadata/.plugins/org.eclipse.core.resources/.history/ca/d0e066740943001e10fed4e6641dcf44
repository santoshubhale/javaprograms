package Testing;

public class ArrayAdditionOfTwoNoEqlToGivenNo {

	public static void main(String[] args) {
		int arr[]= {3,6,7,8,1,2,9};
		int num =10;
		
		for(int i=0; i<arr.length; i++) {
			for(int j=0;j<arr.length; j++) {
				if((arr[i])+(arr[j])==num) {
					System.out.println(arr[i]+" + "+arr[j]+" ="+ num);
				}
			}
		}

	}

}
