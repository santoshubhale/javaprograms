package com.methods;

public class Practice {

	public static void main(String[] args)  {
		try {
		int a=10;
		int b=0;
		System.out.println(a/b);
		}catch(Exception e) {
			System.out.println(e.getMessage());
		}
		String s="santosh";
		System.out.println(s);
		int aa=100;
		int bb=200;
		int c=aa*bb;
		System.out.println(c);
	}
	
}
