package com.finalkey;

class ZXC {

	final int a = 10;
	final int b = 20;

	void show() {
		int a = 15;
		int b = 22;
		ZXC zz = new ZXC();
		System.out.println(zz.a);
		System.out.println(a);
		System.out.println(zz.b);
		System.out.println(b);
	}
	ZXC(){
		int a=44;
		int b=55;
		System.out.println(a);
		System.out.println(b);
	}
}

public class FinalVariableDemo {

	public static void main(String[] args) {
		int a=1;
		int b=2;
		System.out.println(a);
		System.out.println(b);
		ZXC z = new ZXC();
		z.show();
	}

}
