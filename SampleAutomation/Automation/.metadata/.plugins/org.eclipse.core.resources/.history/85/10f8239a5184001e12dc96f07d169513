package com.finalkey;

class ZXC {

	final int a = 10;
	final int b = 20;

	void show() {
		int a = 15;
		int b = 22;
		System.out.println(a);
		System.out.println(b);
	}
	ZXC(){
		System.out.println(a);
		System.out.println(b);
	}
}

public class FinalVariableDemo {

	public static void main(String[] args) {
		int a=1;
		int b=2;
		ZXC z = new ZXC();
		z.show();
	}

}
