package com.sgtesting.assignmentday10_1;

public class Ques2 {

	public static void main(String[] args) {
		int count=0;
		for(int i=70; i>=1; i--) {
			if(i%7==0) {
				count++;
			}
		}
		System.out.println("Total count"+count);
		int a[]=new int[count];
		int y=0;
		for(int j=70; j>=1; j--) {
			if(j%7==0) {
				a[y]=j;
				y++;
			}
		}
		for(int k=a.length-1; k>=0; k--) {
			System.out.println(a[k]);
		}
	}

}
